{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": false,
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "exclude": ["node_modules", "/**/__tests__"],
    "lib": ["es2015"]
}
